        </activity>
        <activity android:name=".nextActivity" android:label="Send Requests"/>
        <activity android:name=".thirdActivity" android:label="Inform"/>
        <activity android:name=".fourthActivity" android:label="Change Acquaintances"/>
    </application>

</manifest>